{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Request",
  "description": "Explicit requests from the client to the webview.",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "$type",
        "id"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "getVersion"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "id",
        "js"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "eval"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        },
        "js": {
          "description": "The javascript to evaluate.",
          "type": "string"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "id",
        "title"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "setTitle"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        },
        "title": {
          "description": "The title to set.",
          "type": "string"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "id"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "getTitle"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "id",
        "visible"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "setVisibility"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        },
        "visible": {
          "description": "Whether the window should be visible or hidden.",
          "type": "boolean"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "id"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "isVisible"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "id"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "openDevTools"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "id"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "getSize"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        },
        "include_decorations": {
          "description": "Whether to include the title bar and borders in the size measurement.",
          "default": null,
          "type": [
            "boolean",
            "null"
          ]
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "id",
        "size"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "setSize"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        },
        "size": {
          "description": "The size to set.",
          "allOf": [
            {
              "$ref": "#/definitions/SimpleSize"
            }
          ]
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "id"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "fullscreen"
          ]
        },
        "fullscreen": {
          "description": "Whether to enter fullscreen mode. If left unspecified, the window will enter fullscreen mode if it is not already in fullscreen mode or exit fullscreen mode if it is currently in fullscreen mode.",
          "type": [
            "boolean",
            "null"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "id"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "maximize"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        },
        "maximized": {
          "description": "Whether to maximize the window. If left unspecified, the window will be maximized if it is not already maximized or restored if it was previously maximized.",
          "type": [
            "boolean",
            "null"
          ]
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "id"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "minimize"
          ]
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        },
        "minimized": {
          "description": "Whether to minimize the window. If left unspecified, the window will be minimized if it is not already minimized or restored if it was previously minimized.",
          "type": [
            "boolean",
            "null"
          ]
        }
      }
    },
    {
      "type": "object",
      "required": [
        "$type",
        "html",
        "id"
      ],
      "properties": {
        "$type": {
          "type": "string",
          "enum": [
            "loadHtml"
          ]
        },
        "html": {
          "description": "HTML to set as the content of the webview.",
          "type": "string"
        },
        "id": {
          "description": "The id of the request.",
          "type": "string"
        },
        "origin": {
          "description": "What to set as the origin of the webview when loading html. If not specified, the origin will be set to the value of the `origin` field when the webview was created.",
          "type": [
            "string",
            "null"
          ]
        }
      }
    }
  ],
  "definitions": {
    "SimpleSize": {
      "type": "object",
      "required": [
        "height",
        "width"
      ],
      "properties": {
        "height": {
          "type": "number",
          "format": "double"
        },
        "width": {
          "type": "number",
          "format": "double"
        }
      }
    }
  }
}